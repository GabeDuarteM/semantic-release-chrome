{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noEmit": true,
    "pretty": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
